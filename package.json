{
  "name": "goose-weather",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-environment": "cp src/environments/local_environment.ts src/environments/environment.ts",
    "build-local": "npm run copy-environment && npm run build",
    "serve-local": "npm run copy-environment && ng serve",
    "coverage-report": "ng test --code-coverage --watch=true",
    "delete-coverage": "rm -rf coverage",
    "code-coverage": "npm run delete-coverage && npm run coverage-report",
    "build-prod": "ng build --prod",
    "test-prod": "ng test --watch=false",
    "firebase-deploy": "firebase deploy",
    "environment-variables": "python scripts/environment_variables.py $OPEN_WEATHER_MAP_API_KEY"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@ngrx/effects": "^9.0.0",
    "@ngrx/schematics": "^9.0.0",
    "@ngrx/store": "^9.0.0",
    "@ngrx/store-devtools": "^9.0.0",
    "@ngxd/core": "^9.0.4",
    "bootstrap": "^4.2.1",
    "core-js": "^3.6.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "puppeteer": "^2.1.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "chai": "^4.2.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-mocha": "^1.3.0",
    "mocha": "^7.1.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3"
  }
}
